<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	
	<typeAliases>
		<typeAlias type="com.kh.thenolja.member.model.vo.Member" alias="member" />
		<typeAlias type="com.kh.thenolja.hotel.model.vo.Hotel" alias="hotel" />
		<typeAlias type="com.kh.thenolja.hotel.model.vo.Room" alias="room" />
		<typeAlias type="com.kh.thenolja.reservation.model.vo.Reservation" alias="reser" />
		<typeAlias type="com.kh.thenolja.refund.model.vo.Refund" alias="refund" />
		<typeAlias type="com.kh.thenolja.reservation.model.vo.Coupon" alias="coupon" />
	</typeAliases>
		
		
	<mappers>
		<mapper resource="/mapper/member-mapper.xml" />
		<mapper resource="/mapper/hotel-mapper.xml" />
		<mapper resource="/mapper/reser-mapper.xml" />
		<mapper resource="/mapper/refund-mapper.xml" />
	</mappers>

</configuration>
